package traversingDemo;

import java.util.ArrayList;

public class Demo {
	public static void main(String[] args) {
		
		ArrayList<Integer> al = new ArrayList<Integer>();
		al.add(10);
		al.add(20);
		al.add(30);
		
		for(int i=0;i<al.size()-1;i++) {
			System.out.println(al.get(i));
		}
		for(Integer l :al) {
			System.out.println(l);
		}
	}

}
