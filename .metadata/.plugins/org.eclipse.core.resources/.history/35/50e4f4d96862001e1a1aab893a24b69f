package Com;

class Employee {
	Employee(int id , String name ){
		this(2.5);
		System.out.println("id :" + id + " name :" +name);
		}
	Employee(int age){
		
		System.out.println("age :" +age );
		
	}
	 Employee(double sal){
		 this(20);
		 System.out.println("sal :" + sal);
	 }
	  	public static void main(String[] args) {
			 new Employee(100, "TOM");
		}
}
