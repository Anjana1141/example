package DefaultSorting;


class Car implements Comparable<Car>{
	
	String brand;
	int cost;
	String color;
	
	Car(String brand, int cost , String color){
		this.brand=brand;
		this.cost=cost;
		this.color=color;
		
	}
	public String toString() {
		return brand+" brand "+cost+" cost "+color+" color ";
	}
	public int compareTo(Car c) {
		return this.cost-c.cost;
	}

}
