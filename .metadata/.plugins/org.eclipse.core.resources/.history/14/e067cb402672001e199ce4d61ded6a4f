package Practise;

public class Student {
	Student(int age){
		System.out.println("age   : " +age);
	}
	Student(String name){
		System.out.println("name   : " +name);
	}
Student(double height){
	System.out.println("height   : " +height);
}
public static void main(String[] args) {
	Student s = new Student(5);
	
}

}
